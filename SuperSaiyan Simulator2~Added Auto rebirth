while true do

local args = {
    [1] = game:GetService("Players").LocalPlayer.leaderstats["\240\159\140\128Rebirth"] -- if you don't want to to auto rebirth then delete line 3-5
}

game:GetService("ReplicatedStorage").RemoteEvents.Rebirth:FireServer(unpack(args))

game:GetService("ReplicatedStorage").Remotes.Combat:FireServer()

game:GetService("ReplicatedStorage").Remotes.Combat2:FireServer()

game:GetService("ReplicatedStorage").Remotes.Combat3:FireServer()

game:GetService("ReplicatedStorage").Remotes.Combat4:FireServer()

game:GetService("ReplicatedStorage").Remotes.Combat5:FireServer()

game:GetService("ReplicatedStorage").Remotes.Combat6:FireServer()

game:GetService("ReplicatedStorage").Remotes.Combat6:FireServer()

wait()
end
